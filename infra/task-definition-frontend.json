{
  "family": "saas-frontend",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::957597318409:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::957597318409:role/ecsTaskRole",
  "containerDefinitions": [
    {
      "name": "nginx",
      "image": "<WILL_BE_REPLACED_BY_GA>",
      "essential": true,
      "portMappings": [
        { "containerPort": 80, "hostPort": 80, "protocol": "tcp" }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/saas-frontend",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ]
}
